{
  "swagger" : "2.0",
  "info" : {
    "description" : "This is a sample Petstore server.  You can find \nout more about Swagger at \n[http://swagger.io](http://swagger.io) or on \n[irc.freenode.net, #swagger](http://swagger.io/irc/).\n",
    "version" : "1.0.0",
    "title" : "Swagger Petstore",
    "termsOfService" : "http://swagger.io/terms/",
    "contact" : {
      "email" : "apiteam@swagger.io"
    },
    "license" : {
      "name" : "Apache 2.0",
      "url" : "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  },
  "host" : "virtserver.swaggerhub.com",
  "basePath" : "/venthur/debbugs/1.0.0",
  "schemes" : [ "https", "http" ],
  "paths" : {
    "/status" : {
      "get" : {
        "summary" : "Get status of given bugs",
        "consumes" : [ "application/json" ],
        "produces" : [ "application/json" ],
        "parameters" : [ {
          "in" : "body",
          "name" : "body",
          "description" : "List of bug numbers",
          "required" : true,
          "schema" : {
            "type" : "array",
            "items" : {
              "type" : "integer"
            }
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Status of the given bugs",
            "schema" : {
              "type" : "array",
              "items" : {
                "$ref" : "#/definitions/Status"
              }
            }
          }
        }
      }
    },
    "/bugs" : {
      "get" : {
        "summary" : "Get bugs that match the conditions",
        "consumes" : [ "application/json" ],
        "produces" : [ "application/json" ],
        "parameters" : [ {
          "in" : "body",
          "name" : "body",
          "description" : "filter conditions",
          "required" : true,
          "schema" : {
            "$ref" : "#/definitions/Conditions"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "List of bugnumbers",
            "schema" : {
              "type" : "array",
              "items" : {
                "type" : "integer"
              }
            }
          }
        }
      }
    },
    "/usertags" : {
      "get" : {
        "summary" : "Get bugs tagged by that email. All of them or only those tagged with the selected tags.",
        "consumes" : [ "application/json" ],
        "produces" : [ "application/json" ],
        "parameters" : [ {
          "in" : "body",
          "name" : "body",
          "description" : "Filter condition for usertags",
          "required" : true,
          "schema" : {
            "$ref" : "#/definitions/body"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "List of bugnumbers",
            "schema" : {
              "type" : "array",
              "items" : {
                "type" : "integer"
              }
            }
          }
        }
      }
    },
    "/latest/{amount}" : {
      "get" : {
        "summary" : "Get list of latest bugs",
        "produces" : [ "application/json" ],
        "parameters" : [ {
          "name" : "amount",
          "in" : "path",
          "description" : "Number of bugs",
          "required" : true
        } ],
        "responses" : {
          "200" : {
            "description" : "List of bugnumbers",
            "schema" : {
              "type" : "array",
              "items" : {
                "type" : "integer"
              }
            }
          }
        }
      }
    },
    "/buglog/{bugnumber}" : {
      "get" : {
        "summary" : "Get the whole bug log",
        "produces" : [ "application/json" ],
        "parameters" : [ {
          "name" : "bugnumber",
          "in" : "path",
          "description" : "the bugnumber",
          "required" : true
        } ],
        "responses" : {
          "200" : {
            "description" : "the the whole bug log",
            "schema" : {
              "$ref" : "#/definitions/Buglog"
            }
          }
        }
      }
    }
  },
  "definitions" : {
    "Status" : {
      "type" : "object",
      "properties" : {
        "bug_number" : {
          "type" : "integer",
          "description" : "the bug number"
        },
        "severity" : {
          "$ref" : "#/definitions/Severity"
        },
        "tags" : {
          "type" : "array",
          "description" : "tags of the bugreport",
          "items" : {
            "type" : "string"
          }
        },
        "subject" : {
          "type" : "string",
          "description" : "subject/title of the bugreport"
        },
        "originator" : {
          "type" : "string",
          "description" : "submitter of the bugreport"
        },
        "mergedwith" : {
          "type" : "array",
          "description" : "this bug was merged with",
          "items" : {
            "type" : "integer"
          }
        },
        "package" : {
          "type" : "string",
          "description" : "package of the bugreport"
        },
        "source" : {
          "type" : "string",
          "description" : "source package of the bugreport"
        },
        "date" : {
          "type" : "string",
          "format" : "date-time",
          "description" : "date of bug creation"
        },
        "log_modified" : {
          "type" : "string",
          "format" : "date-time",
          "description" : "date of last update"
        },
        "done" : {
          "type" : "string",
          "description" : "maintainer who closed the string"
        },
        "archived" : {
          "type" : "boolean",
          "description" : "the bug is archived"
        },
        "unarchived" : {
          "type" : "boolean",
          "description" : "the bug has been unarchived and can be archived again"
        },
        "fixed_versions" : {
          "type" : "array",
          "description" : "version numbers",
          "items" : {
            "type" : "string"
          }
        },
        "found_versions" : {
          "type" : "array",
          "description" : "version numbers",
          "items" : {
            "type" : "string"
          }
        },
        "forwarded" : {
          "type" : "string",
          "description" : "an URL or email address"
        },
        "blocks" : {
          "type" : "array",
          "description" : "list of bugnumbers this report blocks",
          "items" : {
            "type" : "integer"
          }
        },
        "blockedby" : {
          "type" : "array",
          "description" : "list of bugnumbers this report is blocked by",
          "items" : {
            "type" : "integer"
          }
        },
        "pending" : {
          "type" : "string",
          "description" : "either pending or done",
          "enum" : [ "pending", "done" ]
        },
        "msgid" : {
          "type" : "string",
          "description" : "message ID of the bugreport"
        },
        "owner" : {
          "type" : "string",
          "description" : "default empty, otherwise who is responsible for fixing"
        },
        "location" : {
          "type" : "string",
          "description" : "always db-h or archive",
          "enum" : [ "db-h", "archive" ]
        },
        "affects" : {
          "type" : "array",
          "description" : "package names, see affects in control-BTS manual",
          "items" : {
            "type" : "string"
          }
        },
        "summary" : {
          "type" : "string",
          "description" : "arbitrary text"
        }
      },
      "description" : "Status of a bugreport",
      "example" : {
        "severity" : { },
