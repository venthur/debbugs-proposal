    "title" : "Swagger Petstore",
    "termsOfService" : "http://swagger.io/terms/",
    "contact" : {
      "email" : "apiteam@swagger.io"
    },
    "license" : {
      "name" : "Apache 2.0",
      "url" : "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  },
  "host" : "virtserver.swaggerhub.com",
  "basePath" : "/venthur/debbugs/1.0.0",
  "tags" : [ {
    "name" : "pet",
    "description" : "Everything about your Pets",
    "externalDocs" : {
      "description" : "Find out more",
      "url" : "http://swagger.io"
    }
  }, {
    "name" : "store",
    "description" : "Access to Petstore orders"
  }, {
    "name" : "user",
    "description" : "Operations about user",
    "externalDocs" : {
      "description" : "Find out more about our store",
      "url" : "http://swagger.io"
    }
  } ],
  "schemes" : [ "https", "http" ],
  "paths" : {
    "/status" : {
      "get" : {
        "summary" : "Get status of given bugs",
        "consumes" : [ "application/json" ],
        "produces" : [ "application/json" ],
        "parameters" : [ {
          "in" : "body",
          "name" : "body",
          "description" : "List of bug numbers",
          "required" : true,
          "schema" : {
            "type" : "array",
            "items" : {
              "type" : "integer"
            }
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Status of the given bugs",
            "schema" : {
              "type" : "array",
              "items" : {
                "$ref" : "#/definitions/Status"
              }
            }
          }
        }
      }
    }
  },
  "definitions" : {
    "Status" : {
      "type" : "object",
      "properties" : {
        "bug_number" : {
          "type" : "integer",
          "description" : "the bug number"
        },
        "severity" : {
          "type" : "string",
          "description" : "severity of the bug",
          "enum" : [ "critical", "grave", "serious", "important", "normal", "minor", "wishlist" ]
        },
        "tags" : {
          "type" : "array",
          "description" : "tags of the bugreport",
          "items" : {
            "type" : "string"
          }
        },
        "subject" : {
          "type" : "string",
          "description" : "subject/title of the bugreport"
        },
        "originator" : {
          "type" : "string",
          "description" : "submitter of the bugreport"
        },
        "mergedwith" : {
          "type" : "array",
          "description" : "this bug was merged with",
          "items" : {
            "type" : "integer"
          }
        },
        "package" : {
          "type" : "string",
          "description" : "package of the bugreport"
        },
        "source" : {
          "type" : "string",
          "description" : "source package of the bugreport"
        },
        "date" : {
          "type" : "string",
          "format" : "date-time",
          "description" : "date of bug creation"
        },
        "log_modified" : {
          "type" : "string",
          "format" : "date-time",
          "description" : "date of last update"
        },
        "done" : {
          "type" : "string",
          "description" : "maintainer who closed the string"
        },
        "archived" : {
          "type" : "boolean",
          "description" : "the bug is archived"
        },
        "unarchived" : {
          "type" : "boolean",
          "description" : "the bug has been unarchived and can be archived again"
        }
      },
      "description" : "Status of a bugreport",
      "example" : {
        "severity" : "critical",
        "date" : "2000-01-23T04:56:07.000+00:00",
        "package" : "package",
        "subject" : "subject",
        "originator" : "originator",
        "source" : "source",
        "mergedwith" : [ 6, 6 ],
        "done" : "done",
        "tags" : [ "tags", "tags" ],
        "unarchived" : true,
        "bug_number" : 0,
        "archived" : true,
        "log_modified" : "2000-01-23T04:56:07.000+00:00"
      }
    }
  },
  "externalDocs" : {
    "description" : "Find out more about Swagger",
    "url" : "http://swagger.io"
  }
}