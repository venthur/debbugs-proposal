        "owner" : "owner",
        "summary" : "summary",
        "package" : "package",
        "subject" : "subject",
        "blocks" : [ 1, 1 ],
        "pending" : "pending",
        "msgid" : "msgid",
        "affects" : [ "affects", "affects" ],
        "originator" : "originator",
        "source" : "source",
        "blockedby" : [ 5, 5 ],
        "mergedwith" : [ 6, 6 ],
        "done" : "done",
        "forwarded" : "forwarded",
        "tags" : [ "tags", "tags" ],
        "unarchived" : true,
        "bug_number" : 0,
        "archived" : true,
        "log_modified" : "2000-01-23T04:56:07.000+00:00",
        "fixed_versions" : [ "fixed_versions", "fixed_versions" ],
        "location" : "db-h",
        "found_versions" : [ "found_versions", "found_versions" ]
      }
    },
    "Conditions" : {
      "type" : "object",
      "properties" : {
        "package" : {
          "type" : "string"
        },
        "submitter" : {
          "type" : "string"
        },
        "maint" : {
          "type" : "string"
        },
        "src" : {
          "type" : "string"
        },
        "severity" : {
          "$ref" : "#/definitions/Severity"
        },
        "status" : {
          "type" : "string",
          "enum" : [ "open", "done", "forwarded" ]
        },
        "tag" : {
          "type" : "string"
        },
        "owner" : {
          "type" : "string"
        },
        "bugs" : {
          "type" : "string"
        },
        "affects" : {
          "type" : "string"
        },
        "archive" : {
          "type" : "string",
          "enum" : [ "both" ]
        }
      },
      "description" : "Filter conditions",
      "example" : {
        "severity" : { },
        "owner" : "owner",
        "submitter" : "submitter",
        "bugs" : "bugs",
        "package" : "package",
        "src" : "src",
        "maint" : "maint",
        "affects" : "affects",
        "archive" : "both",
        "tag" : "tag",
        "status" : "open"
      }
    },
    "Severity" : {
      "type" : "string",
      "description" : "Bug severity",
      "enum" : [ "critical", "grave", "serious", "important", "normal", "minor", "wishlist" ]
    },
    "Buglog" : {
      "type" : "array",
      "description" : "The whole bug log",
      "items" : {
        "$ref" : "#/definitions/Buglog_inner"
      }
    },
    "body" : {
      "type" : "object",
      "properties" : {
        "email" : {
          "type" : "string",
          "description" : "email address of the tag creator"
        },
        "tags" : {
          "type" : "array",
          "description" : "list of usertags",
          "items" : {
            "type" : "string"
          }
        }
      }
    },
    "Buglog_inner" : {
      "properties" : {
        "header" : {
          "type" : "string",
          "description" : "the smtp headers"
        },
        "body" : {
          "type" : "string",
          "description" : "the email body"
        }
      }
    }
  },
  "externalDocs" : {
