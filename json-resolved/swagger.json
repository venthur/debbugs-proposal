{
  "swagger" : "2.0",
  "info" : {
    "description" : "This is a sample Petstore server.  You can find \nout more about Swagger at \n[http://swagger.io](http://swagger.io) or on \n[irc.freenode.net, #swagger](http://swagger.io/irc/).\n",
    "version" : "1.0.0",
    "title" : "Swagger Petstore",
    "termsOfService" : "http://swagger.io/terms/",
    "contact" : {
      "email" : "apiteam@swagger.io"
    },
    "license" : {
      "name" : "Apache 2.0",
      "url" : "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  },
  "host" : "virtserver.swaggerhub.com",
  "basePath" : "/venthur/debbugs/1.0.0",
  "tags" : [ {
    "name" : "pet",
    "description" : "Everything about your Pets",
    "externalDocs" : {
      "description" : "Find out more",
      "url" : "http://swagger.io"
    }
  }, {
    "name" : "store",
    "description" : "Access to Petstore orders"
  }, {
    "name" : "user",
    "description" : "Operations about user",
    "externalDocs" : {
      "description" : "Find out more about our store",
      "url" : "http://swagger.io"
    }
  } ],
  "schemes" : [ "https", "http" ],
  "paths" : {
    "/status" : {
      "get" : {
        "summary" : "Get status of given bugs",
        "consumes" : [ "application/json" ],
        "produces" : [ "application/json" ],
        "parameters" : [ {
          "in" : "body",
          "name" : "body",
          "description" : "List of bug numbers",
          "required" : true,
          "schema" : {
            "type" : "array",
            "items" : {
              "type" : "integer"
            }
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Status of the given bugs",
            "schema" : {
              "type" : "array",
              "items" : {
                "$ref" : "#/definitions/Status"
              }
            }
          }
        }
      }
    }
  },
  "definitions" : {
    "Status" : {
      "type" : "object",
      "properties" : {
        "bug_number" : {
          "type" : "integer",
          "description" : "the bug number"
        },
        "severity" : {
          "type" : "string",
          "description" : "severity of the bug",
          "enum" : [ "critical", "grave", "serious", "important", "normal", "minor", "wishlist" ]
        },
        "tags" : {
          "type" : "array",
          "description" : "tags of the bugreport",
          "items" : {
            "type" : "string"
          }
        },
        "subject" : {
          "type" : "string",
          "description" : "subject/title of the bugreport"
        }
      },
      "example" : {
        "severity" : "critical",
        "bug_number" : 0,
        "subject" : "subject",
        "tags" : [ "tags", "tags" ]
      }
    },
    "Order" : {
      "type" : "object",
      "properties" : {
        "id" : {
          "type" : "integer",
          "format" : "int64"
        },
        "petId" : {
          "type" : "integer",
          "format" : "int64"
        },
        "quantity" : {
          "type" : "integer",
          "format" : "int32"
        },
        "shipDate" : {
          "type" : "string",
          "format" : "date-time"
        },
        "status" : {
          "type" : "string",
          "description" : "Order Status",
          "enum" : [ "placed", "approved", "delivered" ]
        },
        "complete" : {
          "type" : "boolean",
          "default" : false
        }
      },
      "xml" : {
        "name" : "Order"
      }
    }
  },
  "externalDocs" : {
    "description" : "Find out more about Swagger",
    "url" : "http://swagger.io"
  }
}